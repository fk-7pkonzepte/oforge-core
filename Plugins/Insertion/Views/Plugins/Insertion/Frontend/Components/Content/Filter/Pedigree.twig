<div class="tag" data-key="{{ f }}" data-value="{{ v }}">
    <a href="#"
       onclick="window.location.href = Oforge.updateQueryString('{{ f }}[search_ancestor_1]', null, Oforge.updateQueryString('{{ f }}[search_ancestor_2]'))"
       class="close"></a>
    <span class="tag__text">
            {{ i18n(f) }}
        {% set unit = i18n('unit_' ~  attribute.name, '') %}
        <span class="tag__text">
            {% for val in value %}
                {% if loop.first == false %}
                    {{ i18n('and') }}
                {% endif %}
                {{ val }}

            {% endfor %}
        </span>
</span>
</div>
